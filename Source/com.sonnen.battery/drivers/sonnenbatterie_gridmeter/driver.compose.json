    "meter_power.exportedDaily": {
      "title": {
        "en": "∑d Grid feed-in",
        "de": "∑d Einspeisung",
        "nl": "∑d Roosterinvoer"
      },
      "icon": "/assets/images/utility-pole-thin-grid-feed-in_daily.svg"
    },
