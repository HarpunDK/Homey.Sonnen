      "title": {
        "en": "∑ Grid feed-in",
        "de": "∑ Einspeisung",
        "nl": "∑ Roosterinvoer"
      },
      "icon": "/assets/images/utility-pole-thin-grid-feed-in_daily.svg"
    }
